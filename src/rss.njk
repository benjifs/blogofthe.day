---
permalink: /feed.xml
---
<?xml version="1.0" encoding="UTF-8"?>
<rss xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:content="http://purl.org/rss/1.0/modules/content/" xmlns:atom="http://www.w3.org/2005/Atom" version="2.0">
    <channel>
        <title><![CDATA[Blog of the .Day]]></title>
        <description><![CDATA[Blog of the .Day]]></description>
        <link>https://blogofthe.day</link>
        <atom:link href="https://blogofthe.day/feed.xml" rel="self" type="application/rss+xml"/>
        <lastBuildDate>{% dateISO %}</lastBuildDate>
        {%- for date, site in dates.dates %}
        <item>
            <title><![CDATA[{{ site }}]]></title>
            <description><![CDATA[{{ sites.descriptions[site] | safe }}]]></description>
            <link>https://{{ site }}</link>
            <guid isPermaLink="true">https://{{ site }}</guid>
            <pubDate>{{ date }}</pubDate>
        </item>
        {%- endfor %}
    </channel>
</rss>